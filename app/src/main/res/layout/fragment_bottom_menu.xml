<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coBack"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.tpn.baro.Fragment.BottomMenu">
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:id="@+id/real_bottom"
            android:layout_width="match_parent"
            android:layout_height="@dimen/design_bottom_navigation_height"
            android:background="@color/white"
            app:labelVisibilityMode="labeled"
            app:itemIconTint="@drawable/bottom_navigation_select_color"
            app:itemTextColor="@drawable/bottom_navigation_select_color"
            app:itemRippleColor="@color/invisible"
            app:itemIconSize="16dp"
            app:itemTextAppearanceActive="@style/bottom_text_style"
            app:itemTextAppearanceInactive="@style/bottom_text_style"
            app:menu="@menu/bottom_menu"
            tools:ignore="PrivateResource" />
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</FrameLayout>